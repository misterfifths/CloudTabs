{
  "key": "__BASE64_KEY__",
  
  "name": "Safari CloudTabs",
  "short_name": "CloudTabs",
  "version": "__VERSION__",
  "manifest_version": 2,
  "description": "Fetch tabs from your mobile iOS devices.",
  "homepage_url": "https://github.com/misterfifths/CloudTabs",
  "icons": {
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/newtab.html"
  },
  "background": {
    "scripts": [ "src/background/background.js" ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "nativeMessaging",
    "alarms"
  ]
}
