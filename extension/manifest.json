{
  "key": "",
  
  "name": "Safari CloudTabs",
  "short_name": "CloudTabs",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Fetch tabs from your mobile iOS devices.",
  "homepage_url": "https://github.com/misterfifths/CloudTabs",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/newtab.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "nativeMessaging"
  ]
}
